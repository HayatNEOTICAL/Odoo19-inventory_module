<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_transaction_list_view" model="ir.ui.view">
        <field name="name">stock.transactions</field>
        <field name="model">stock.transactions</field>
        <field name="arch" type="xml">
            <list string="Stock Transaction">
                <field name="product_id"/>
                <field name="qty"/>
                <field name="move_type"/>
                <field name="date"/>
                <field name="references"/>            
            </list>
        </field>
    </record>

    <record id="stock_transaction_form_view" model="ir.ui.view">
        <field name="name">stock.transactions</field>
        <field name="model">stock.transactions</field>
        <field name="arch" type="xml">
            <form string="Stock Move">
                <sheet>
                    <group class = "Stock Transaction">
                        <field name="product_id"/>
                        <field name="qty"/>
                        <field name="move_type"/>
                        <field name="date"/>
                        <field name="references"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
